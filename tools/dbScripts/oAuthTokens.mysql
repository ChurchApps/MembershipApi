CREATE TABLE `oAuthTokens` (
  `id` char(11) NOT NULL,
  `clientId` char(11) DEFAULT NULL,
  `userChurchId` char(11) DEFAULT NULL,
  `accessToken` varchar(45) DEFAULT NULL,
  `refreshToken` varchar(45) DEFAULT NULL,
  `scopes` varchar(45) DEFAULT NULL,
  `accessTokenExpiresAt` datetime DEFAULT NULL,
  `refreshTokenExpiresAt` datetime DEFAULT NULL,
  `createdAt` datetime DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB;
