DROP TABLE IF EXISTS `clientErrors`;

CREATE TABLE `clientErrors` (
  `id` char(11) NOT NULL,
  `application` varchar(45) DEFAULT NULL,
  `errorTime` datetime DEFAULT NULL,
  `userId` char(11) DEFAULT NULL,
  `churchId` char(11) DEFAULT NULL,
  `originUrl` varchar(255) DEFAULT NULL,
  `errorType` varchar(45) DEFAULT NULL,
  `message` varchar(255) DEFAULT NULL,
  `details` text,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB;
