DROP TABLE IF EXISTS `userChurches`;

CREATE TABLE `userChurches` (
  `id` char(11) NOT NULL,
  `userId` char(11) DEFAULT NULL,
  `churchId` char(11) DEFAULT NULL,
  `personId` char(11) DEFAULT NULL,
  `lastAccessed` datetime DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `userId` (`userId`),
  KEY `churchId` (`churchId`)
) ENGINE=InnoDB;
